type User {
    id: ID!,
    firstName: String!,
    lastName: String!,
    dob: String,
    email: String,
    phone: String
}

type Query {
    users(limit: Int):[User],
    user(id: ID):User
}

input UserMutationInput {
    id: ID!,
    email: String!,
    phone: String
}

type Mutation {
    createUser(firstName: String!, lastName: String!, dob: String, email: String, phone: String = "NA"):User
    updateUser(userMutationInput: UserMutationInput):User
}